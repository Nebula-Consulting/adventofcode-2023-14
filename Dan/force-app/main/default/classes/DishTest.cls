        Dish dish = new Dish(
                'O....#....\n' +
                        'O.OO#....#\n' +
                        '.....##...\n' +
                        'OO.#O....O\n' +
                        '.O.....O#.\n' +
                        'O.#..O.#.#\n' +
                        '..O..#O..O\n' +
